// --- KHOI TAO ---
304A    // ADDI $r1, $r0, 10   ; Op=3, Rs=0, Rt=1 (001), Imm=10 -> R1=10
3083    // ADDI $r2, $r0, 3    ; Op=3, Rs=0, Rt=2 (010), Imm=3  -> R2=3

// --- TINH TOAN ALU0 ---
0298    // ADDU $r3, $r1, $r2  ; R3 = 10 + 3 = 13 (0x000D)
02A1    // SUBU $r4, $r1, $r2  ; R4 = 10 - 3 = 7  (0x0007)

// --- MULT/DIV (Ghi vao HI/LO) ---
0282    // MULTU $r1, $r2      ; LO = 30, HI = 0
0283    // DIVU $r1, $r2       ; LO = 3,  HI = 1

// --- LOGIC ---
02AC    // AND  $r5, $r1, $r2  ; R5 = 10 & 3 = 2  (0x0002)
02B5    // OR   $r6, $r1, $r2  ; R6 = 10 | 3 = 11 (0x000B)
02BE    // NOR  $r7, $r1, $r2  ; R7 = ~(11)  = -12 (0xFFF4)
028F    // XOR  $r1, $r1, $r2  ; R1 = 10 ^ 3 = 9  (0x0009)

// --- KET THUC ---
F000    // HLT